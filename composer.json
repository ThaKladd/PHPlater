{
    "name": "phplater/phplater",
    "description": "A simple PHP template engine that lets PHP do most the logic and then append it to the HTML in the template file.",
    "keywords": [
        "PHP",
        "Template",
        "Templating",
        "Template engine",
        "tpl",
        "PHP templates",
        "Templates"
    ],
    "homepage": "https://github.com/ThaKladd/PHPlater",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "files": ["src/functions.php"]
    },
    "authors": [
        {
            "name": "John Larsen",
            "email": "johninen@gmail.com",
            "homepage": "https://betweenmovies.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5",
        "vimeo/psalm": "5.x-dev",
        "phpstan/phpstan": "1.8.x-dev"
    }
}
